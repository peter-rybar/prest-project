{
    "compilerOptions": {
        "module": "commonjs",
        "target": "es5",
        "noImplicitAny": false,
        "removeComments": false,
        "preserveConstEnums": true,
        "strictNullChecks": false,
        "sourceMap": false,
        "outDir": "build"
    },
    "include": [
        "./src/app.ts",
        "./src/server.ts",
        "./src/cluster.ts"
    ],
    "exclude": [
        "node_modules"
    ]
}