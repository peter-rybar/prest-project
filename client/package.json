{
  "name": "project",
  "scripts": {
    "tslint": "tslint --project tsconfig.json --type-check",
    "build": "tsc",
    "build:watch": "npm run build -- -w",
    "predist": "npm run clean && npm run tslint && npm run build && npm run test",
    "dist": "rollup-multiple -c",
    "rollup": "rollup -c",
    "rollup-multiple": "rollup-multiple -c",
    "dist:watch": "npm run dist -- -w",
    "test": "node ./node_modules/karma-cli/bin/karma start ./karma.conf.js --single-run",
    "test:watch": "node ./node_modules/karma-cli/bin/karma start ./karma.conf.js",
    "clean": "rimraf build; rimraf dist",
    "clean:all": "npm run clean; rimraf node_modules"
  },
  "devDependencies": {
    "@types/assertion-error": "^1.0.30",
    "@types/chai": "^3.4.34",
    "@types/jquery": "^2.0.41",
    "@types/mocha": "^2.2.33",
    "chai": "^3.5.0",
    "karma": "^1.3.0",
    "karma-chai": "^0.1.0",
    "karma-chrome-launcher": "^1.0.1",
    "karma-cli": "^1.0.1",
    "karma-mocha": "^1.3.0",
    "karma-phantomjs-launcher": "^1.0.2",
    "karma-requirejs": "^1.1.0",
    "mocha": "^3.1.2",
    "requirejs": "^2.3.2",
    "rimraf": "^2.5.4",
    "rollup": "^0.36.3",
    "rollup-multiple": "^0.41.6",
    "rollup-plugin-babel": "^2.6.1",
    "rollup-plugin-replace": "^1.1.1",
    "rollup-plugin-typescript": "^0.8.1",
    "rollup-plugin-uglify": "^1.0.1",
    "rollup-watch": "^2.5.0",
    "source-map-support": "^0.4.15",
    "tslint": "3.15.1",
    "typescript": "2.3.x"
  },
  "dependencies": {
    "incremental-dom": "^0.5.1"
  }
}
